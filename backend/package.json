{
  "name": "fixtime-ai-backend",
  "version": "1.0.0",
  "description": "Backend API for FixTime AI CMMS",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "d1:create": "wrangler d1 create fixtime-db",
    "d1:migrate:local": "wrangler d1 execute fixtime-db --local --file=./schema.sql",
    "d1:migrate:prod": "wrangler d1 execute fixtime-db --remote --file=./schema.sql"
  },
  "dependencies": {
    "hono": "^4.6.0",
    "jose": "^5.9.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240314.0",
    "wrangler": "^3.78.0"
  }
}